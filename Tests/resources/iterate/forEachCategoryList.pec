define print as native method receiving Text value doing:
	Java: System.out.print(value);
	C#: System.Console.Write(value);
	Python2: print(objects=value,end="")
	Python3: print(objects=value,end="")
	JavaScript: process.stdout.write(value);

define name as Text attribute
define text as Text attribute

define Parent as category with attribute name
define Child as Parent with attribute text

define convertIterator as method receiving Child[] children returning Iterator<Parent> doing:
	return ( Parent from child ) for each child in children

define filterList as method receiving Child[] children returning Child[] doing:
	return children filtered with child where child.name = "a"
	
define main as method receiving Text{} options doing:
	children = [ Child with "a" as name, Child with "b" as name ]
	filtered = filterList with children as children
	converted = convertIterator with filtered as children
	for each c3 in converted:
		print c3.name