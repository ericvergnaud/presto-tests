attribute path: Text;

native resource MyResource( path ) {

	category bindings {
		Java: prompto.runtime.utils.MyResource;
		C#: prompto.runtime.utils.MyResource;
		Python2: MyResource from module: prompto.runtime.utils.MyResource;
		Python3: MyResource from module: prompto.runtime.utils.MyResource;
		JavaScript: MyResource from module: test/user/MyResource.js;
	}
}
		
native method print ( Text value ) {
	Java: System.out.print(value);
	C#: System.Console.Write(value);
	Python2: print(objects=value,end="")
	Python3: print(objects=value,end="")
	JavaScript: process.stdout.write(value);
}

method main ( Text{} options ) {
	with ( file = MyResource ( path = "xyz8" ) )
		write ( "writePartialOk") to file;
	s = read all from MyResource ( path = "xyz8" );
	print (s);
}
		
