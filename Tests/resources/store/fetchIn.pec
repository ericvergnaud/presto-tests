define print as native method receiving Text value doing:
	Java: System.out.print(value);
	C#: System.Console.Write(value);
	Python2: print(objects=value,end="")
	Python3: print(objects=value,end="")
	JavaScript: process.stdout.write(value);

// ok to use Integer with MemStore
define dbId as Integer attribute 

define name as storable Text attribute with index

define Entity as storable category with attribute name

define main as method receiving Text<:> options doing:
	e1 = Entity with "John" as name
	e2 = Entity with "Lucy" as name
	store e1, e2
	fetched = fetch all Entity where name in ["John", "Janet"]
	listed = fetched.toList
	print "/count:" + listed.count + "/name:" + listed[1].name
	fetched = fetch all Entity where name in ["John", "Lucy"] order by name
	listed = fetched.toList
	print "/count:" + listed.count + "/name:" + listed[1].name